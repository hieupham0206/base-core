{
  "name": "cloudteam/basecore",
  "description": "Cloudteam Base Core",
  "license": "MIT",
  "authors": [
    {
      "name": "Phạm Quang Hiếu",
      "email": "hieu.pham@cloudteam.vn"
    }
  ],
  "homepage": "https://github.com/hieupham0206/basecore",
  "keywords": [
    "Laravel",
    "Cloudteam"
  ],
  "require": {
    "illuminate/support": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "^1.1",
    "orchestra/testbench": "~3.0",
    "sempro/phpunit-pretty-print": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Cloudteam\\BaseCore\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Cloudteam\\BaseCore\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Cloudteam\\BaseCore\\BaseCoreServiceProvider"
      ],
      "aliases": {
        "BaseCore": "Cloudteam\\BaseCore\\Facades\\BaseCore"
      }
    }
  }
}
